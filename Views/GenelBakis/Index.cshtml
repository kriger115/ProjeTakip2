
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- Content -->


<div class="row">
    <div class="col-xxl-8 mb-6 order-0">
        <div class="card">
            <div class="d-flex align-items-start row">
                <div class="col-sm-7">
                    <div class="card-body">
                        <h5 class="card-title text-primary mb-3">Proje Atama Programına Hoşgeldiniz!  🎉</h5>
                        <p class="mb-6">
                            Toplam <span class="fw-bold">@ViewBag.ProjeSayisi</span> proje mevcuttur hepsini detaylı olarak inceleyebilirsiniz.
                        </p>

                        <a href="/PersonelProje/Index/" class="btn btn-sm btn-outline-primary">Detayları İncele</a>
                    </div>
                </div>
                <div class="col-sm-5 text-center text-sm-left">
                    <div class="card-body pb-0 px-0 px-md-6">
                        <img src="~/TEMA/assets/img/illustrations/man-with-laptop.png" height="175" class="scaleX-n1-rtl" alt="View Badge User">
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-4 col-md-4 order-1">
        <div class="row">
            <div class="col-lg-6 col-md-12 col-6 mb-6">
                <div class="card h-100">
                    <div class="card-body">
                        <div class="card-title d-flex align-items-start justify-content-between mb-4">
                            <div class="avatar flex-shrink-0">
                                <img src="~/TEMA/assets/img/icons/unicons/chart-success.png" alt="chart success" class="rounded">
                            </div>

                        </div>
                        <span class="fw-semibold d-block mb-1">Tamamlanan Projeler</span>

                        <small class="text-success fw-semibold"><i class="bx bx-up-arrow-alt"></i> @ViewBag.TamamlanmisProje Tamamlandı</small>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-md-12 col-6 mb-6">
                <div class="card h-100">
                    <div class="card-body">
                        <div class="card-title d-flex align-items-start justify-content-between mb-4">
                            <div class="avatar flex-shrink-0">
                                <img src="~/TEMA/assets/img/icons/unicons/wallet-info.png" alt="wallet info" class="rounded">
                            </div>

                        </div>
                        <span>Yüksek Öncelikli</span>

                        <small class="text-success fw-semibold"><i class="bx bx-up-arrow-alt"></i> @ViewBag.YuksekOncelikli Proje</small>
                    </div>
                </div>
            </div>
        </div>
    </div>
